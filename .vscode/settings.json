{
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.experimental.classRegex": ["clsx([^]*)"],

  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "node_modules": false,
    ".vscode": false,
    ".eslintrc.json": false,
    "next-env.d.ts": false,
    ".gitignore": false,
    "next.config.js": false,
    "package.json": false,
    "postcss.config.js": false,
    "README.md": false,
    "tsconfig.json": false,
    "yarn.lock": false,
    ".next": false,
    "package-lock.json": false,
    "tsconfig.tsbuildinfo": false,
    "studio/node_modules": false,
    "studio/package.json": false,
    "studio/README.md": false,
    "studio/tsconfig.json": false,
    "studio/config/.checksums": false,
    "studio/static": false,
